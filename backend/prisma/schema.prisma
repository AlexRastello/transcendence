generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id Int @id @default(autoincrement())
  createdAt DateTime @default(now())
  id_user Int @unique
	email String
	login String
	first_name String
	last_name String
	img_link String
  connected Boolean @default(false)
}
